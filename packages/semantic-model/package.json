{
  "name": "@vscode-ui5/semantic-model",
  "version": "0.0.1",
  "description": "Semantic model provider for UI5",
  "keywords": [],
  "files": [
    "lib/src",
    "api.d.ts",
    "src",
    "resources"
  ],
  "main": "lib/src/api.js",
  "repository": "https://github.com/sap/ui5-vscode/",
  "license": "Apache-2.0",
  "typings": "./api.d.ts",
  "dependencies": {
    "lodash": "4.17.15",
    "@vscode-ui5/semantic-model-types": "0.0.1",
    "deep-freeze-strict": "1.1.1",
    "ajv": "6.12.0"
  },
  "devDependencies": {
    "fs-extra": "8.1.0",
    "@types/fs-extra": "8.0.1",
    "@vscode-ui5/test-utils": "0.0.1",
    "@types/deep-freeze-strict": "1.1.0",
    "json-schema-to-typescript": "8.1.0"
  },
  "scripts": {
    "ci": "npm-run-all generate-json-api compile lint coverage",
    "clean": "rimraf ./lib",
    "compile": "yarn run clean && tsc -p .",
    "compile:watch": "tsc -p . --watch",
    "lint": "eslint . --ext .ts --max-warnings=0 --ignore-path=../../.gitignore",
    "test": "mocha",
    "coverage": "nyc mocha",
    "generate-json-api": "json2ts -i ./resources/sap-ui-library-api.json -o ./src/api-json.d.ts --bannerComment \"/* eslint-disable @typescript-eslint/no-explicit-any */ /* This file is automatically generated. DO NOT MODIFY IT BY HAND. Instead, modify the source JSONSchema file (sap-ui-library-api.json) and run generate-json-api from the package.json to regenerate this file. */\""
  },
  "publishConfig": {
    "access": "public"
  }
}
