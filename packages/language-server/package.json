  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@xml-tools/parser": "1.0.3",
    "@xml-tools/ast": "4.0.2",
    "vscode-languageserver": "6.1.1",
    "vscode-languageserver-textdocument": "1.0.0",
    "node-fetch": "2.6.0",
    "@ui5-editor-tools/semantic-model": "0.0.1",
    "@ui5-editor-tools/xml-views-completion": "0.0.1",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@types/node-fetch": "2.5.5",
