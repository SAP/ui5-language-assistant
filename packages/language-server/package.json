{
  "name": "@ui5-editor-tools/language-server",
  "version": "0.0.1",
  "description": "UI5 Language Server",
  "keywords": [
    "LSP",
    "language-server",
    "UI5",
    "OpenUI5",
    "SAPUI5"
  ],
  "files": [
