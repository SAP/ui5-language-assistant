  "license": "Apache-2.0",
  "typings": "./api.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@ui5-language-assistant/semantic-model": "^1.4.0",
    "@ui5-language-assistant/settings": "^0.1.0",
    "@ui5-language-assistant/xml-views-completion": "^1.5.0",
    "@ui5-language-assistant/xml-views-tooltip": "^1.2.0",
    "@ui5-language-assistant/xml-views-validation": "^0.5.0",
    "@xml-tools/ast": "4.2.0",
    "@xml-tools/ast-position": "2.0.0",
    "@xml-tools/parser": "1.0.6",
    "assert-never": "1.2.1",
    "fs-extra": "9.0.1",
    "lodash": "4.17.15",
    "node-fetch": "2.6.0",
    "vscode-languageserver": "6.1.1",
    "vscode-languageserver-textdocument": "1.0.1"
  },
  "devDependencies": {
    "@types/node-fetch": "2.5.7",
    "@types/tmp": "0.2.0",
    "@ui5-language-assistant/semantic-model-types": "^1.3.0",
    "@ui5-language-assistant/test-utils": "^1.4.2",
    "tmp-promise": "3.0.2",
    "vscode-languageserver-types": "3.15.1"
