    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@ui5-language-assistant/semantic-model": "^1.0.0",
    "@xml-tools/parser": "1.0.3",
    "chevrotain": "6.5.0"
  },
  "scripts": {
    "ci": "npm-run-all clean compile lint coverage",
    "clean": "rimraf ./lib ./coverage ./nyc_output",
