    "api.d.ts",
    "src"
  ],
  "main": "lib/src/api.js",
  "repository": "https://github.com/sap/ui5-language-assistant/",
  "license": "Apache-2.0",
  "typings": "./api.d.ts",
  "dependencies": {
    "@ui5-language-assistant/logic-utils": "^1.0.0",
    "@xml-tools/ast": "4.0.2",
